{
  "name": "marvin-delta-to-markdown",
  "version": "0.0.5",
  "public": true,
  "description": "Convert from Quill Delta to Markdown which is understandable by Slack Social Media Platform",
  "main": "index.js",
  "engines": {
    "node": ">=6.4.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "mocha \"tests/**/*.spec.js\" --reporter progress --watch",
    "pretty": "find ./**/*.js | xargs prettier --write --no-semi --print-width 80 --single-quote --trailing-comma es5"
  },
  "keywords": [
    "quill",
    "slack",
    "delta",
    "markdown"
  ],
  "author": "Marvin team",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-node": "^8.0.1",
    "jest": "^23.6.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "lodash": "^4.16.4"
  }
}
